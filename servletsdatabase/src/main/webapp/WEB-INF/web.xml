  	<servlet-class>Customerview.view</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Customerservlet</servlet-name>
  	<url-pattern>/addcustomer.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>view</servlet-name>
  	<url-pattern>/success.view</url-pattern>
  </servlet-mapping>
