<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CustomerController</servlet-name>
  	<display-name>CustomerController</display-name>
  	<description></description>
  	<servlet-class>controller.CustomerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HomeController</servlet-name>
  	<display-name>HomeController</display-name>
  	<description></description>
  	<servlet-class>controller.HomeController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerView</servlet-name>
  	<display-name>CustomerView</display-name>
  	<description></description>
  	<servlet-class>view.CustomerView</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Customerupdatecontr</servlet-name>
  	<display-name>Customerupdatecontr</display-name>
  	<description></description>
  	<servlet-class>controller.Customerupdatecontr</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Updatecontroller</servlet-name>
  	<display-name>Updatecontroller</display-name>
  	<description></description>
  	<servlet-class>controller.Updatecontroller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Deletecustomercont</servlet-name>
  	<display-name>Deletecustomercont</display-name>
  	<description></description>
  	<servlet-class>controller.Deletecustomercont</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CustomerController</servlet-name>
  	<url-pattern>/controller.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HomeController</servlet-name>
  	<url-pattern>/homecontroller.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerView</servlet-name>
  	<url-pattern>/success.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Customerupdatecontr</servlet-name>
  	<url-pattern>/customerUpdate.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Updatecontroller</servlet-name>
  	<url-pattern>/updateCustomer.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Deletecustomercont</servlet-name>
  	<url-pattern>/deletecustomer.do</url-pattern>
  </servlet-mapping>
</web-app>
